{
  "name": "@synthetixio/synpress",
  "version": "4.0.10",
  "homepage": "https://synpress.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/Synthetixio/synpress.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./types/index.d.ts"
    },
    "./cypress": {
      "types": "./types/cypress/index.d.ts",
      "default": "./dist/cypress/index.js"
    },
    "./cypress/support": {
      "types": "./types/cypress/support/index.d.ts",
      "default": "./dist/cypress/support/index.js"
    },
    "./playwright": {
      "types": "./types/playwright/index.d.ts",
      "default": "./dist/playwright/index.js"
    }
  },
  "bin": "./dist/cli.js",
  "files": [
    "README.md",
    "dist",
    "src",
    "types"
  ],
  "dependencies": {
    "@synthetixio/ethereum-wallet-mock": "0.0.11",
    "@synthetixio/synpress-cache": "0.0.11",
    "@synthetixio/synpress-metamask": "0.0.11",
    "@synthetixio/synpress-core": "0.0.11"
  },
  "devDependencies": {
    "@synthetixio/synpress-tsconfig": "0.0.11",
    "@types/node": "20.11.17",
    "rimraf": "5.0.5",
    "tsup": "8.0.2",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "@playwright/test": "*"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run build:dist && pnpm run build:types",
    "build:dist": "tsup",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf dist types",
    "lint": "biome check . --apply",
    "lint:check": "biome check . --verbose",
    "lint:unsafe": "biome check . --apply-unsafe",
    "types:check": "tsc --noEmit"
  }
}